{
  "extends": "react-app",
  "rules": {
    "space-before-function-paren": [
      "error",
      {
        "anonymous": "never",
        "named": "never",
        "asyncArrow": "never"
      }
    ],
    "arrow-parens": 0,
    "generator-star-spacing": 0,
    "no-console": ["warn", { "allow": ["warn", "error"] }],
    "semi": ["error", "always"]
  }
}
